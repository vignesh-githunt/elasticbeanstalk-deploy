{
  "manifest_version": 2,
  "name": "XDR - The Sales Development Robot",
  "description": "The XDR is a personal Sales/Business/Market/Account Development Representative that works for you.",
  "background": {
    "scripts": ["static/js/background.js"]
  },
  "browser_action": {
    "default_icon": {
      "144": "icons/logo@144.png"
    }
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+X"
      }
    }
  },
  "content_scripts": [
    {
      "css": ["static/css/content.css"],
      "js": ["static/js/content.js"],
      "matches": ["<all_urls>"]
    }
  ],
  "content_security_policy": "script-src 'self' 'unsafe-eval' https://github.githubassets.com https://*.pusher.com https://api.mixpanel.com https://cdnjs.cloudflare.com; object-src 'self' chrome-extension://omkfaingifmnipkahnfblnebpbpfkfak/static/css/content.css.map chrome-extension://omkfaingifmnipkahnfblnebpbpfkfak/static/js/content.js.map chrome-extension://omkfaingifmnipkahnfblnebpbpfkfak/static/js/background.js.map",
  "externally_connectable": {
    "matches": [
      "*://*.outboundworks.com/*",
      "*://*.koncert.com/*",
      "http://localhost:5006/*"
    ]
  },
  "icons": {
    "144": "icons/logo@144.png",
    "16": "icons/logo@16.png",
    "32": "icons/logo@32.png"
  },
  "key": "",
  "optional_permissions": ["https://*/*"],
  "options_ui": {
    "page": "options.html"
  },
  "permissions": ["cookies", "notifications", "storage"],
  "short_name": "XDR",
  "update_url": "https://clients2.google.com/service/update2/crx",
  "version": "1.0.6",
  "web_accessible_resources": ["/static/css/content.css", "/static/media/*"]
}
